{
  "name": "@dr-kobros/hardcore-react-training",
  "version": "4.0.0",
  "description": "A training / starter project utilizing Broilerplate & React",
  "main": "src",
  "scripts": {
    "build:client": "cross-env NODE_ENV=production webpack --progress --display-optimization-bailout",
    "build:server": "cross-env NODE_ENV=production webpack --progress --config webpack.server.config.js --display-optimization-bailout",
    "build:node": "cross-env NODE_ENV=production babel src/index.js > dist-server/index.js",
    "build": "yarn run build:client && yarn run build:server && yarn run build:node",
    "lint": "eslint src",
    "babel-node": "babel-node",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js --port 8888 --history-api-fallback --hot",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pekkis/react-training-broilerplate"
  },
  "keywords": [
    "React",
    "example",
    "starter",
    "boilerplate",
    "broilerplate",
    "training"
  ],
  "author": "Pekkis",
  "license": "BSD-3-Clause",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(pcss)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "apollo-boost": "^0.1.27",
    "apollo-client": "^2.4.12",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "faker": "^4.1.0",
    "formik": "^1.5.0",
    "graphql": "^14.1.1",
    "identity-obj-proxy": "^3.0.0",
    "immutable": "^4.0.0-rc.12",
    "luxon": "^1.11.2",
    "polished": "^2.3.3",
    "prop-types": "^15.7.1",
    "random-js": "^1.0.8",
    "react": "16.8.1",
    "react-apollo": "^2.4.1",
    "react-dom": "^16.8.1",
    "react-error-boundary": "^1.2.3",
    "react-helmet": "^5.2.0",
    "react-immutable-proptypes": "^2.1.0",
    "react-inspector": "^2.3.1",
    "react-loadable": "^5.4.0",
    "react-pose": "^4.0.7",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-typography": "^0.16.18",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-promise-middleware": "^6.0.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.1.2",
    "transit-immutable-js": "^0.7.0",
    "transit-js": "^0.8.861",
    "typography": "^0.16.18",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-flow-strip-types": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.3.1",
    "@dr-kobros/broilerplate": "^0.23.0",
    "@dr-kobros/broilerplate-mini-css-extract": "^0.23.0",
    "@dr-kobros/broilerplate-postcss": "^0.23.0",
    "@dr-kobros/broilerplate-styled-components": "^0.23.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.x",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.9.2",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-react-app": "^3.0.7",
    "eslint-plugin-flowtype": "2.x",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.1.0",
    "normalize.css": "^8.0.1",
    "postcss-advanced-variables": "^3.0.0",
    "postcss-import": "^12.0.1",
    "postcss-modules": "^1.4.1",
    "postcss-nested": "^4.1.0",
    "postcss-nesting": "^7.0.0",
    "prettier": "^1.16.4",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.10"
  }
}
